---
import Layout from "@layouts/Layout.astro";
import TitlePage from "@components/page-components/profil/title-page.astro";
---

<Layout title="Profil BRIDA NTB" menu={2}>
  <TitlePage
    slot="hero"
    title="Sekretaris Badan Riset dan Inovasi Daerah NTB"
  />
  <div class="bg-white py-6 sm:py-8 lg:py-12 flex flex-col gap-5 items-center">
    <div class="flex flex-col items-center">
      <div
        class="w-32 h-32 md:w-52 md:h-52 bg-slate-200 rounded-full overflow-hidden shadow-lg mb-2 md:mb-4"
      >
        <img
          src="/foto-team/Sekban.jpg"
          loading="lazy"
          alt="Photo by Radu Florin"
          class="w-full h-full object-cover object-center"
        />
      </div>
      <p class="text-blue-500 font-bold text-center md:text-left">
        Sekretaris Badan
      </p>
      <h1
        class="text-gray-800 text-2xl sm:text-3xl font-bold text-center md:text-left mb-4 md:mb-6"
      >
        Retno Untari, S.Si, M.Kes
      </h1>
    </div>
    <div class="rounded-lg flex flex-col items-center w-full">
      <h2
        class="text-gray-800 text-xl sm:text-2xl font-semibold text-left md:text-center mb-2 md:mb-4"
      >
        Biodata :
      </h2>
      <div
        class="flex gap-5 justify-center w-full flex-wrap flex-col md:flex-row"
      >
        <div
          class="justify-center border p-5 rounded-md flex-1 text-left sm:text-center flex flex-col items-center"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            viewBox="0 0 576 512"
            class="w-[40px] h-[40px]"
